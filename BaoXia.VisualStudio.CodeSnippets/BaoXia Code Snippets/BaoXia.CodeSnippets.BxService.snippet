			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
			<Shortcut>BxServiceParamsDefine_CurrentUserId_ClientIpInfo_OperateTime</Shortcut>
		</Header>
		<Snippet>
			<Code Language="CSharp">
