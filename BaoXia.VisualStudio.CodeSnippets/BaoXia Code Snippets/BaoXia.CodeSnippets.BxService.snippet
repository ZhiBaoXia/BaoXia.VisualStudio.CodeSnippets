				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
			<Shortcut>BxServiceParamsFill_CurrentUserId_ClientIpInfo_OperateTime_Null</Shortcut>
		</Header>
		<Snippet>
			<Code Language="CSharp">
